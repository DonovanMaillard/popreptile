],
"display_list": [
  "cd_nom",
  "nombre_compte",
  "nombre_estime_min",
  "nombre_estime_max"
],
  "specific": {
    "cd_nom": {
      "type_widget": "taxonomy",
      "attribut_label": "Espèce observée",
      "keyValue": "cd_nom",
      "keyLabel": "__MODULE.TAXONOMY_DISPLAY_FIELD_NAME",
      "multiple": false,
      "api": "taxref/allnamebylist/__MODULE.ID_LIST_TAXONOMY",
      "application": "TaxHub",
      "required": true,
      "type_util": "taxonomy"
    } ,
    "id_nomenclature_stade": {
      "type_widget": "nomenclature",
      "attribut_label": "Stade",
      "code_nomenclature_type": "STADE_VIE_PR",
	    "required": true,
      "type_util": "nomenclature"  
    },
    "abondance": {
      "type_widget": "checkbox",
      "attribut_label": "Abondance",
      "values": ["Nombre d'individus observés à vue", "Nombre d'individus observés sur/sous plaques"]
    },
    "type_denombrement": {
      "type_widget": "radio",
      "attribut_label": "Précision du dénombrement",
      "values": ["Compté", "Estimé"]
    },
    "nombre_compte": {
      "type_widget": "number",
      "attribut_label": "Nombre (compté)",
      "required": false,
      "min": 0,
      "max": 100,
      "hidden": "({value}) => value.type_denombrement != 'Compté'"
    },
    "nombre_estime_min": {
      "type_widget": "number",
      "attribut_label": "Nombre minimal (estimé)",
	    "required": false,
      "hidden": "({value}) => value.type_denombrement != 'Estimé'"
    },
    "nombre_estime_max": {
      "type_widget": "number",
      "attribut_label": "Nombre maximal (estimé)",
      "required": false,
      "hidden": "({value}) => value.type_denombrement != 'Estimé'"
    },
    "comments": {
      "type_widget": "textarea",
      "attribut_label": "Commentaire (sexe des individus observés etc)"
    }
  }
}
